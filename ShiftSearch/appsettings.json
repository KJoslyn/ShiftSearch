{
    "Serilog": {
        "Using": ["Serilog.Sinks.File", "Serilog.Sinks.Console"],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning", // Microsoft log messages must be warning level or higher
                "System": "Warning"
            }
        },
        "Enrich": ["MachineName", "FromLogContext", "WithProcessId", "WithThreadId"],
        "WriteTo": [
            { "Name": "Console" },
            {
                "Name": "File",
                "Args": {
                    "path": "C:\\Users\\Admin\\WindowsServices\\ShiftSearch\\ShiftSearch\\logs\\log-.txt",
                    "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exception:1}",
                    "rollingInterval": "Day"
                }
            },
            {
                "Name": "File",
                "Args": {
                    "path": "C:\\Users\\Admin\\WindowsServices\\ShiftSearch\\ShiftSearch\\logs\\log-.json",
                    "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                    "rollingInterval": "Day"
                }
            },
            {
                "Name": "Seq",
                "Args": {
                    //"serverUrl": "http://localhost:8081"
                    "serverUrl": "http://localhost:5341"
                }
            }
        ]
    },
    "Symbols": [
        {
            "Symbol": "VXX",
            "Url": "https://shiftsearch.com/stock/VXX?tt=B&t=past+5+days",
            "PutThresholds": [ 300000, 1000000 ],
            "CallThresholds": [ 300000, 1000000 ]
        }
    ],
    "Plivo": {
        "From": "+14077836727"
    }
}